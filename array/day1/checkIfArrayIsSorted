#include <iostream>
using namespace std;

int main(){
    int arr[13]={1,2,3,4,5,5,8,1,0,7,55,87,44};

    bool isSorted = true; // non-decreasing order
    for(int i = 1; i < 13; i++){
        if(arr[i-1] > arr[i]){
            isSorted = false;
            break;
        }
    }
    cout << boolalpha << isSorted << '\n';
    return 0;
}